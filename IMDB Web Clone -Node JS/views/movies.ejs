<%- include('_header'); -%>
    <section>
        <div class="row">
            <% for(var i=0; i< data.length; i++){%>
                <% if(data[i]["ib_poster"]!=null && data[i]["ib_poster"].length>5){%>
                    <div class="col-2 cardDesign">
                        <div class="card">
                            <a href="/info?id=<%= data[i]['ib_imdbid']%>">
                                <div class="card-body content">
                                    <div class="content-overlay"> </div>
                                    <img src="<%= data[i]['ib_poster']%>"
                                        width="100%" height="250px">
                                    <div class="content-details fadeIn-bottom">
                                        <h3 class="content-title" style="font-size: 14px;">
                                            <%=data[i]['ib_moviename'] %>
                                        </h3>
                                    </div>
                                </div>
                            </a>
                        </div>
                    </div>
                    <%} else{%>
                        <div class="col-2 cardDesign">
                            <div class="card">
                                <a href="/info?id=<%= data['results'][i]['id']%>">
                                    <div class="card-body content">
                                        <div class="content-overlay"> </div>
                                        <img src="images/no_image_available.jpg" width="100%" height="250px">
                                        <div class="content-details fadeIn-bottom">
                                            <h3 class="content-title" style="font-size: 14px;">
                                                <%=data[i]['ib_moviename'] %>
                                            </h3>
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </div>
                        <%} %>
                            <%} %>
        </div>
    </section>
    <%- include ('_footer'); -%>